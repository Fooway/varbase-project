{
  "name": "vardot/varbase-project",
  "description": "Varbase Project",
  "type": "project",
  "license": "GPL-2.0+",
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.2.0",
    "wikimedia/composer-merge-plugin": "~1.4"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "merge-plugin": {
      "include": [
        "composer.varbase.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": true,
      "merge-extra-deep": false,
      "merge-scripts": true
    }
  }
}
